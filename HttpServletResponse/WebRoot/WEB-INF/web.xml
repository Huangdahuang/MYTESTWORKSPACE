<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>Response</servlet-name>
    <servlet-class>com.huang.response.Response</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SessionDemo1</servlet-name>
    <servlet-class>com.itheima.servlet.SessionDemo1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SessionDemo2</servlet-name>
    <servlet-class>com.itheima.servlet.SessionDemo2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Request</servlet-name>
    <servlet-class>com.huang.request.Request</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>aServlet</servlet-name>
    <servlet-class>转发和包含.AServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>bServlet</servlet-name>
    <servlet-class>转发和包含.BServlet</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>Response</servlet-name>
    <url-pattern>/servlet/response</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SessionDemo1</servlet-name>
    <url-pattern>/servlet/sessionDemo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SessionDemo2</servlet-name>
    <url-pattern>/servlet/sessionDemo2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Request</servlet-name>
    <url-pattern>/servlet/request</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>aServlet</servlet-name>
    <url-pattern>/servlet/aServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>bServlet</servlet-name>
    <url-pattern>/servlet/bServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
